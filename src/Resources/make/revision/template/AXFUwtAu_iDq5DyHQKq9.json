    "body": "{% extends '@EMSCH\/template\/base.html.twig' %}\r\n{% trans_default_domain trans_default_domain %}\r\n\r\n{% block canonical %}{% endblock %}\r\n\r\n\r\n{% block title %}{{ 'error.title'|trans }}{% endblock %}\r\n\r\n{% block body %}\r\n    <main role=\"main\" class=\"container mt-5 mb-5\" id=\"main-content\">\r\n        <div class=\"row\">\r\n            <div class=\"col-md-8 offset-md-2\">\r\n                <div class=\"alert alert-danger\" role=\"alert\">\r\n                    <h1><i class=\"fa fa-danger text-yellow\"><\/i> {{ 'error.title'|trans }}<\/h1>\r\n                    <p>\r\n                        {{ 'error.description'|trans }}\r\n                    <\/p>\r\n                    <form class=\"search-form\">\r\n                        <div class=\"input-group\">\r\n                            <input type=\"text\" name=\"search\" class=\"form-control\" placeholder=\"{{ 'action.search'|trans|e('html_attr') }}\" aria-label=\"{{ 'action.search'|trans|e('html_attr') }}\" name=\"q\">\r\n                            <div class=\"input-group-append\">\r\n                                <button type=\"submit\" name=\"submit\" class=\"btn btn-danger btn-flat\">\r\n                                    <i class=\"fa fa-search\"><\/i>\r\n                                    {{ 'action.search'|trans }}\r\n                                <\/button>\r\n                            <\/div>\r\n                        <\/div>\r\n                    <\/form>\r\n                <\/div>\r\n            <\/div>\r\n        <\/div>\r\n    <\/main>\r\n{% endblock %}",
